    "license": ["GPL-2.0+"],
	"keywords": ["TYPO3 CMS", "Shopware", "API"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
